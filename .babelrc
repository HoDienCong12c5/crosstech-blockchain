{
  "presets": [
    [
      "@babel/preset-react"
    ],
    [
      "next/babel"
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-arrow-functions",
    [
      "@babel/plugin-syntax-dynamic-import"
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ]
  ],
  "env": {
